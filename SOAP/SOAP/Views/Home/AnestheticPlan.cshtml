@{
    ViewBag.Title = "Anesthetic Plan";
}
            <form action="" name="AnestheticPlan">
            <div class="left half">
            <h2>Anesthetic Plan</h2>
            <h4>Premedication</h4>
                <div class="input">
                    <label>Route of Administration: </label>
                    <select name="PreMedRoute" id="Patient.AnestheticPlanPremedication.Route" subgroup="PreMedications">
					</select>
                </div>
                <div class="input">
                    <label>Sedative Drugs: </label>
                    <select name="PreMedDrugs"  id="Patient.AnestheticPlanPremedication.SedativeDrug" subgroup="PreMedications">
					</select>
                </div>
               <div class="input">
			       <label>Dosage: </label>
                   <input type="text" name="PreMedDosage" class="inline" id="Patient.AnestheticPlanPremedication.SedativeDosage" subgroup="PreMedications"/>
                </div>
                <div class="input">
                    <label>Opioid Drugs: </label>
                    <select name="PreMedDrugs"  id="Patient.AnestheticPlanPremedication.OpioidDrug" subgroup="PreMedications">
					</select>
                </div>
               <div class="input">
			       <label>Dosage: </label>
                   <input type="text" name="PreMedDosage" class="inline" id="Patient.AnestheticPlanPremedication.OpioidDosage" subgroup="PreMedications"/>
                </div>
                <div class="input">
                    <label>Anticholinergic Drugs: </label>
                    <select name="PreMedDrugs"  id="Patient.AnestheticPlanPremedication.AnticholinergicDrug" subgroup="PreMedications">
					</select>
                </div>
               <div class="input">
			       <label>Dosage: </label>
                   <input type="text" name="PreMedDosage" class="inline" id="Patient.AnestheticPlanPremedication.AnticholinergicDosage" subgroup="PreMedications"/>
                </div>
            </div>
            <div class="right half">
            <div style="margin-top: 115px"></div>
               <div class="input">
			       <label>Ketamine Dosage: </label>
                   <input type="text" name="PreMedDosage" class="inline" id="Patient.AnestheticPlanPremedication.KetamineDosage" subgroup="PreMedications"/>
                </div>                
            
            </div>
             </form>